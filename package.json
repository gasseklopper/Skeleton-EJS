{
  "name": "skeleton-ejs",
  "version": "4.0.0",
  "description": "A front-end template build upon Rosid",
  "private": true,
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/Skeleton.git"
  },
  "license": "MIT",
  "scripts": {
    "preinstall": "bower install",
    "start": "rosid serve src/ -o",
    "compile": "rosid compile src/ dist/ -i '**/_*' -i '**/.bower.json' -i '**/assets/includes' -i '**/*.data.{js,json}'"
  },
  "dependencies": {
    "basiclightbox": "^3.0.4",
    "rosid": "^9.0.1",
    "rosid-handler-ejs": "^8.0.2",
    "rosid-handler-js": "^10.0.1",
    "rosid-handler-sass": "^5.0.2"
  }
}
